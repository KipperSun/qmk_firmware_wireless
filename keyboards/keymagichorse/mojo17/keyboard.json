{
    "keyboard_name": "Mojo17",
    "manufacturer": "LJTuring",
    "maintainer": "LJTuring",
    "processor": "STM32F411",
    "bootloader": "tinyuf2",
    // "bootloader": "stm32-dfu",
    "diode_direction": "COL2ROW",
    "usb": {
        "vid": "0x5123",
        "pid": "0xA028",
        "device_version": "1.0.0",
        "force_nkro": true
    },
    "build": {
        "debounce_type": "sym_defer_pk",
        "lto": true
    },
    "dynamic_keymap": {
        "layer_count": 4
    },
    "features": {
        "nkro": true,
        "bootmagic": true,
        "extrakey": true,
        "mousekey": true,
        "via": true,
        "bluetooth": true,
        "rgb_matrix": true
    },
    "bootmagic": {
        "enabled": true,
        "matrix": [4, 0]
    },
    "bluetooth":{
        "driver": "bhq"
    },
    "ws2812": {
        "pin": "B10",
        "driver": "pwm"
    },
    "rgb_matrix": {
        "driver": "ws2812",
        "default": {
            "on": true,
            "animation": "cycle_up_down",
            "val": 127,
            "speed": 63
        },
        "animations": {
            "alphas_mods": true,
            "gradient_up_down": true,
            "gradient_left_right": true,
            "breathing": true,
            "band_sat": true,
            "band_val": true,
            "band_pinwheel_sat": true,
            "band_pinwheel_val": true,
            "band_spiral_sat": true,
            "band_spiral_val": true,
            "cycle_all": true,
            "cycle_left_right": true,
            "cycle_up_down": true,
            "rainbow_moving_chevron": true,
            "cycle_out_in": true,
            "cycle_out_in_dual": true,
            "cycle_pinwheel": true,
            "cycle_spiral": true,
            "dual_beacon": true,
            "rainbow_beacon": true,
            "rainbow_pinwheels": true,
            "raindrops": true,
            "jellybean_raindrops": true,
            "hue_breathing": true,
            "hue_pendulum": true,
            "hue_wave": true,
            "pixel_fractal": true,
